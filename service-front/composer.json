{
    "name": "ministryofjustice/opg-lpa-front",
    "description": "Front end service for creating Lasting Power of Attorney PDF documents",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "https://github.com/ministryofjustice/opg-lpa",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/townxelliot/ZfcTwig",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/bfoosness/laminas-mvc-plugin-flashmessenger",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/townxelliot/laminas-console",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/laminas/laminas-mvc-console",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/fezfez/laminas-test",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/Slamdunk/laminas-cache",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/townxelliot/laminas-form/",
            "no-api": true
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.3",
        "aws/aws-sdk-php": "^3.91",
        "ministryofjustice/opg-lpa-datamodels": "dev-tech_php_8_support",
        "michelf/php-markdown": "^1.8",
        "alphagov/pay-integration-php": "^1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "soundasleep/html2text": "^0.5.0",
        "laminas/laminas-component-installer": "^2.1.1",
        "laminas/laminas-cache": "dev-php_80_support",
        "laminas/laminas-mvc": "^3.1.1",
        "laminas/laminas-authentication": "^2.7",
        "laminas/laminas-form": "dev-php8",
        "laminas/laminas-session": "^2.9.3",
        "laminas/laminas-math": "^3.2.0",
        "laminas/laminas-crypt": "^3.3.1",
        "laminas/laminas-i18n": "^2.12",
        "kokspflanze/zfc-twig": "dev-php8",
        "laminas/laminas-mail": "^2.12.0",
        "laminas/laminas-json": "^3.1.2",
        "sendgrid/sendgrid": "^6.2",
        "laminas/laminas-console": "dev-php8 as 2.6",
        "laminas/laminas-mvc-console": "^1.3",
        "theiconic/php-ga-measurement-protocol": "^2.9",
        "laminas/laminas-zendframework-bridge": "^1.1.1",
        "laminas/laminas-log": "^2.12",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "laminas/laminas-test": "dev-php8"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    }
}
